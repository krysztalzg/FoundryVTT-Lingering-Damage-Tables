ars.","img":"icons/svg/d20-black.svg","documentId":null,"flags":{}}],"formula":"1d20","replacement":true,"displayRoll":true,"sort":0,"ownership":{"default":0,"lbzgXHSJYDJ01IoT":3},"flags":{},"_stats":{"systemId":"dnd5e","systemVersion":"2.3.1","coreVersion":"10.303","createdTime":1694465397303,"modifiedTime":1694467517944,"lastModifiedBy":"lbzgXHSJYDJ01IoT"},"_id":"7BKX2K5miDK3PM6p"}
